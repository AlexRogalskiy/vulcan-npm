{
  "name": "@vulcanjs/i18n",
  "version": "0.5.3",
  "description": "Vulcan i18n helpers",
  "main": "./dist/index.mjs",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./server": "./dist/server/index.mjs"
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "server": [
        "./dist/server/index.d.ts"
      ]
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:common": "webpack --config ./webpack.config.js",
    "build:server": "webpack --config ./webpack.config.server.js",
    "build:webpack": "yarn run build:common && yarn run build:server",
    "build:tsup": "tsup",
    "build:types": "tsc --emitDeclarationOnly --declaration --project tsconfig.build.json # Tsup generates .d.ts but we still need this for .d.ts.map",
    "build": "yarn run build:tsup"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@vulcanjs/schema": "^0.5.3",
    "@vulcanjs/utils": "^0.5.3",
    "prop-types": "^15.7.2",
    "react-cookie": "^4.1.1",
    "simpl-schema": "^1.12.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@apollo/client": "^3.5.5",
    "react": "^17.0.2"
  },
  "gitHead": "672e9ed494ded98361abe47132899d3f825d6d23"
}
