{
  "name": "@vulcanjs/utils",
  "version": "0.6.1",
  "description": "Vulcan helpers",
  "main": "./dist/index.mjs",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./testing": "./dist/testing.mjs"
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "testing": [
        "./dist/testing.d.ts"
      ]
    }
  },
  "author": "eric-burel <eb@lebrun-burel.com>",
  "homepage": "https://github.com/VulcanJS/vulcan-npm#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VulcanJS/vulcan-npm.git"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "build:webpack": "webpack --config ./webpack.config.js",
    "build:tsup": "tsup",
    "build:types": "tsc --emitDeclarationOnly --declaration --project tsconfig.build.json # Tsup generates .d.ts but we still need this for .d.ts.map",
    "build": "yarn run build:tsup"
  },
  "bugs": {
    "url": "https://github.com/VulcanJS/vulcan-npm/issues"
  },
