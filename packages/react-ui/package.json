{
  "name": "@vulcanjs/react-ui",
  "version": "0.5.1",
  "description": "Vulcan UI components for React",
  "main": "./dist/index.js",
  "files": [
    "dist/"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "yarn run build:webpack # yarn run build:esm && yarn run build:cjs && yarn run build:types",
    "build:esm": "esbuild index.ts --outdir=dist --sourcemap --bundle --minify --platform=neutral --out-extension:.js=.mjs --main-fields=module,main,browser --external:tty --external:graphql --external:react --external:react-dom",
    "build:cjs": "esbuild index.ts --outdir=dist --sourcemap --bundle --minify --platform=node --out-extension:.js=.cjs --external:tty --external:graphql --external:react --external:react-dom",
    "build:webpack": "webpack --config ./webpack.config.js",
    "build:types": "tsc --emitDeclarationOnly --declaration --project tsconfig.build.json"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@vulcanjs/core": "^0.5.1",
    "@vulcanjs/graphql": "^0.5.1",
    "@vulcanjs/i18n": "^0.5.1",
    "@vulcanjs/model": "^0.5.1",
    "@vulcanjs/permissions": "^0.5.1",
    "@vulcanjs/react-hooks": "^0.5.1",
    "@vulcanjs/schema": "^0.5.1",
    "@vulcanjs/utils": "^0.5.1",
    "classnames": "^2.3.1",
    "debug": "^4.3.1",
    "lodash": "^4.17.21",
    "react-bootstrap": "^2.0.3",
    "react-bootstrap-typeahead": "^6.0.0-alpha.4",
    "simpl-schema": "^1.12.0"
  },
  "peerDependencies": {
    "react": ">= 16.13.1",
    "react-dom": ">= 16.13.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@types/underscore": "^1.11.1",
    "graphql": "^15.5.0"
  },
  "gitHead": "ee0303a08e123a54d65af6c36410fa8929f9427f"
}
