{
  "name": "@vulcanjs/crud",
  "version": "0.6.1-alpha.0",
  "main": "./dist/index.js",
  "files": [
    "dist/"
  ],
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./server": "./dist/server/index.js"
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "server": [
        "./dist/server/index.d.ts"
      ]
    }
  },
  "description": "Vulcan graphQL schema generator",
  "author": "eric-burel <eb@lebrun-burel.com>",
  "homepage": "https://github.com/VulcanJS/vulcan-npm#readme",
  "license": "MIT",
  "dependencies": {
    "@vulcanjs/schema": "^0.6.1-alpha.0",
    "debug": "^4.3.1",
    "lodash": "^4.17.21"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VulcanJS/vulcan-npm.git"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "build:common": "esbuild index.ts --outdir=dist --sourcemap --bundle --minify",
    "build:server": "esbuild server/index.ts --outdir=dist/server --sourcemap --bundle --platform=node --target=node12.13",
    "build:esbuild": "yarn run build:common && yarn run build:server",
    "build:webpack": "webpack --config ./webpack.config.js && webpack --config ./webpack.config.server.js # Old build => 3x slower!",
    "build:swc": "spack # TODO: doesn't generate the same structure as webpack or tsc, not yet ok",
    "build:types": "tsc --declaration --emitDeclarationOnly --project ./tsconfig.build.json",
    "build:tsup": "tsup",
    "build": "yarn run build:tsup"
  },
  "bugs": {
    "url": "https://github.com/VulcanJS/vulcan-npm/issues"
  },
  "gitHead": "ee0303a08e123a54d65af6c36410fa8929f9427f",
  "publishConfig": {
    "access": "public"
  },
  "optionalDependencies": {
    "msw": "^0.35.0"
  }
}
