{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "prebuild": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "build": {
      "dependsOn": ["^build", "^build:types", "prebuild"],
      "outputs": ["dist/**", ".next/**"]
    },
    "build:types": {
      "dependsOn": ["^build:types", "^build"],
      "outputs": ["dist/**/*.d.ts", "dist/**/*.d.ts.map"]
    }
  }
}
